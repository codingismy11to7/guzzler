{
  "name": "@guzzler/webui",
  "version": "0.0.4",
  "private": true,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/codingismy11to7/guzzler.git",
    "directory": "packages/webui"
  },
  "scripts": {
    "codegen": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/@types/resources.d.ts",
    "dev": "vite",
    "build": "tsc -b tsconfig.build.json && vite build",
    "check": "tsc -b tsconfig.json",
    "bundle-size": "npx --yes vite-bundle-visualizer",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,tsx,mts}\" --max-warnings=0",
    "lint:fix": "npm run codegen && eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,tsx,mts}\" --max-warnings=0 --fix",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:watch": "vitest --coverage"
  },
  "dependencies": {
    "@effect/platform-browser": "^0.51.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@guzzler/domain": "^0.0.4",
    "@mui/icons-material": "^6.3.1",
    "@mui/lab": "^6.0.0-beta.22",
    "@mui/material": "^6.3.1",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "type-route": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "globals": "^15.14.0",
    "i18next-resources-for-ts": "^1.5.0",
    "vite": "^6.0.5"
  }
}
