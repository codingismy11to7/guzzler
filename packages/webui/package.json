{
  "name": "@guzzlerapp/webui",
  "version": "0.0.6",

  "license": "MIT",
  "private": true,
  "type": "module",

  "repository": {
    "directory": "packages/webui",
    "type": "git",
    "url": "git+ssh://git@github.com/codingismy11to7/guzzler.git"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && vite build",
    "bundle-size": "npx --yes vite-bundle-visualizer",
    "check": "tsc -b tsconfig.json",
    "codegen": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/@types/resources.d.ts",
    "coverage": "vitest run --coverage",
    "coverage:watch": "vitest --coverage",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest"
  },

  "dependencies": {
    "@effect/platform": "^0.90.9",
    "@effect/platform-browser": "^0.70.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@guzzlerapp/domain": "^0.0.6",
    "@guzzlerapp/utils": "^0.0.3",
    "@mui/icons-material": "^7.3.5",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@uidotdev/usehooks": "^2.4.1",
    "date-fns": "^4.1.0",
    "effect": "^3.17.13",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.3.5",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "type-route": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "globals": "^16.4.0",
    "i18next-resources-for-ts": "^1.7.4",
    "vite": "^7.3.0",
    "vite-plugin-svgr": "^4.5.0"
  }
}
